{
  "version": "0.2",
  "language": "en",
  "enableFiletypes": [
    "markdown",
    "mdx",
    "typescript",
    "typescriptreact",
    "javascript",
    "javascriptreact"
  ],
  "ignorePaths": [
    ".cspell.json",
    ".github/workflows/**",
    ".vscode/*.json",
    "**/*.{json,snap}",
    "**/**/CHANGELOG.md",
    "**/**/CONTRIBUTORS.md",
    "**/**/TSLINT_RULE_ALTERNATIVES.md",
    "**/coverage/**",
    "**/dist/**",
    "**/fixtures/**",
    "**/node_modules/**",
    "packages/website/.docusaurus/**",
    "packages/website/build/**",
    "packages/website/src/vendor/**",
    "yarn.lock"
  ],
  "dictionaries": [
    "typescript",
    "softwareTerms",
    "node",
    "en_US",
    "npm",
    "misc",
    "filetypes"
  ],
  "ignoreRegExpList": [
    "/```[\\w\\W]*?```/",
    "/~~~[\\w\\W]*?~~~/",
    "/``[\\w\\W]*?``/",
    "/`[^`]*`/",
    "/\\.\\/docs\\/rules\\/[^.]*.md/",
    "/@typescript-eslint\\/[a-z-]+/",
    "/\\.all-contributorsrc/",
    "/TS[^\\s]+/",
    "\\(#.+?\\)"
