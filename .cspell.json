{
  "version": "0.2",
  "language": "en",
  "enableFiletypes": [
    "markdown",
    "mdx",
    "typescript",
    "typescriptreact",
    "javascript",
    "javascriptreact"
  ],
  "ignorePaths": [
    ".cspell.json",
    ".github/workflows/**",
    ".vscode/*.json",
    "**/*.{json,snap}",
    "**/**/CHANGELOG.md",
    "**/**/CONTRIBUTORS.md",
    "**/**/TSLINT_RULE_ALTERNATIVES.md",
    "**/coverage/**",
    "**/dist/**",
    "**/fixtures/**",
    "**/node_modules/**",
    "packages/website/.docusaurus/**",
    "packages/website/build/**",
    "packages/website/src/vendor/**",
    "yarn.lock"
  ],
  "dictionaries": [
    "typescript",
    "softwareTerms",
    "node",
    "en_US",
    "npm",
    "misc",
    "filetypes"
  ],
  "ignoreRegExpList": [
    "/```[\\w\\W]*?```/",
    "/~~~[\\w\\W]*?~~~/",
    "/``[\\w\\W]*?``/",
    "/`[^`]*`/",
    "/\\.\\/docs\\/rules\\/[^.]*.md/",
    "/@typescript-eslint\\/[a-z-]+/",
    "/\\.all-contributorsrc/",
    "/TS[^\\s]+/",
    "\\(#.+?\\)"
  ],
  "words": [
    "Airbnb",
    "Airbnb's",
    "ambiently",
    "Armano",
    "astexplorer",
    "Astro",
    "ASTs",
    "autofix",
    "autofixers",
    "autofixes",
    "automations",
    "backticks",
    "bigint",
    "bivariant",
    "blockless",
    "blurple",
    "camelcase",
    "Cena",
    "codebases",
    "Codecov",
    "contravariant",
    "Crockford",
    "declarators",
    "destructure",
    "destructured",
    "discoverability",
    "dprint",
    "errored",
    "erroring",
    "ESLint",
    "ESLint's",
    "espree",
    "esquery",
    "esrecurse",
    "estree",
    "falsiness",
    "globby",
    "IDE's",
    "IIFE",
    "IIFEs",
    "linebreaks",
    "lzstring",
    "markdownlint",
    "metastring",
    "necroing",
    "nocheck",
    "noninteractive",
    "nullish",
    "OOM",
    "OOMs",
    "parameterised",
    "performant",
    "pluggable",
    "postprocess",
    "postprocessor",
    "preact",
    "Premade",
    "prettier's",
    "Quickstart",
    "recurse",
    "redeclaration",
    "redeclarations",
    "redeclared",
    "reimplement",
    "resync",
    "ROADMAP",
    "ruleset",
    "rulesets",
    "serializers",
    "Sourcegraph",
    "stringification",
    "superset",
    "thenables",
    "transpiled",
    "transpiles",
    "transpiling",
    "triaging",
    "trpc",
    "tsconfigs",
    "tseslint",
    "tsvfs",
    "typedef",
    "typedefs",
    "unfixable",
    "unoptimized",
    "unprefixed",
    "upsert",
    "warnonunsupportedtypescriptversion",
    "Zacher"
  ],
  "overrides": [
    {
      "filename": "**/*.{ts,mts,cts,js,cjs,mjs,tsx,jsx}",
      "ignoreRegExpList": ["/@[a-z]+/", "/#(end)?region/"],
      "includeRegExpList": [
        "/\\/\\*[\\s\\S]*?\\*\\/|([^\\\\:]|^)\\/\\/.*$/",
        "/(\\/\\/[^\\n\\r]*[\\n\\r]+)/"
      ]
    }
  ]
}
