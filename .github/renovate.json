{
  "automerge": true,
  "enabledManagers": ["github-actions", "npm"],
  "internalChecksFilter": "strict",
  "labels": ["dependencies"],
  "packageRules": [
    {
      "matchManagers": ["npm"],
      "addLabels": ["javascript"]
    },
    {
      "matchManagers": ["github-actions"],
      "addLabels": ["github-actions"]
    }
  ],
  "postUpdateOptions": ["yarnDedupeHighest"],
  "stabilityDays": 3
}
