{
  "name": "@typescript-eslint/website-eslint",
  "private": true,
  "description": "ESLint which works in browsers.",
  "files": [
    "dist",
    "!*.tsbuildinfo"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/typescript-eslint/typescript-eslint.git",
    "directory": "packages/website-eslint"
  },
  "bugs": {
    "url": "https://github.com/typescript-eslint/typescript-eslint/issues"
  },
  "homepage": "https://typescript-eslint.io",
  "license": "MIT",
  "type": "commonjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0"
  },
  "scripts": {
    "//": "These package scripts are mostly here for convenience. Task running is handled by Nx at the root level.",
    "build": "tsx ./build.mts",
    "clean": "rimraf dist/",
    "format": "pnpm -w run format",
    "lint": "pnpm -w exec nx lint",
    "typecheck": "pnpm -w exec nx typecheck"
  },
  "dependencies": {
    "@typescript-eslint/tsconfig-utils": "workspace:*"
  },
  "devDependencies": {
    "@eslint/js": "*",
    "@typescript-eslint/eslint-plugin": "workspace:*",
    "@typescript-eslint/parser": "workspace:*",
    "@typescript-eslint/type-utils": "workspace:*",
    "@typescript-eslint/scope-manager": "workspace:*",
    "@typescript-eslint/tsconfig-utils": "workspace:*",
    "@typescript-eslint/typescript-estree": "workspace:*",
    "@typescript-eslint/visitor-keys": "workspace:*",
    "esbuild": "~0.25.0",
    "eslint": "*",
    "esquery": "*",
    "rimraf": "*",
    "tsx": "*"
  },
  "nx": {
    "name": "website-eslint",
    "includedScripts": [
      "clean",
      "build"
    ]
  }
}
