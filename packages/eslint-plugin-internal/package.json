{
  "name": "@typescript-eslint/eslint-plugin-internal",
  "private": true,
  "main": "dist/index.js",
  "types": "index.d.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/typescript-eslint/typescript-eslint.git",
    "directory": "packages/eslint-plugin-internal"
  },
  "bugs": {
    "url": "https://github.com/typescript-eslint/typescript-eslint/issues"
  },
  "homepage": "https://typescript-eslint.io",
  "license": "MIT",
  "scripts": {
    "build": "pnpm exec nx build",
    "clean": "rimraf dist/ coverage/",
    "format": "pnpm -w run format",
    "lint": "pnpm -w exec nx lint",
    "test": "pnpm -w exec nx test",
    "typecheck": "pnpm -w exec nx typecheck"
  },
  "dependencies": {
    "@typescript-eslint/rule-tester": "workspace:*",
    "@typescript-eslint/scope-manager": "workspace:*",
    "@typescript-eslint/type-utils": "workspace:*",
    "@typescript-eslint/utils": "workspace:*",
    "make-synchronized": "^0.8.0",
    "oxfmt": "^0.31.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "*",
    "rimraf": "*",
    "vitest": "^3.2.4"
  },
  "nx": {
    "name": "eslint-plugin-internal",
    "includedScripts": [
      "clean"
    ],
    "targets": {
      "lint": {
        "command": "eslint"
      }
    }
  }
}
