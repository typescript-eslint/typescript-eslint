  "description": "An ESLint custom parser which leverages TypeScript ESTree",
  "main": "dist/parser.js",
  "version": "21.0.2",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=6.14.0"
  },
