{
  "name": "website",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "root": "packages/website",
  "sourceRoot": "packages/website/src",
  "targets": {
    "start": {
      "dependsOn": [
        {
          "projects": ["website-eslint"],
          "target": "build"
        }
      ],
      "executor": "nx:run-commands",
      "options": {
        "cwd": "{projectRoot}",
        "command": "docusaurus start"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
