{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "NodeNext",
    "allowJs": true,
    "esModuleInterop": true,
    "jsx": "react",
    "lib": ["DOM", "ESNext"],
    "noEmit": true,
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@site/*": ["./*"]
    },
    "types": [
      "@docusaurus/module-type-aliases",
      "@docusaurus/theme-classic",
      "cypress"
    ]
  },
  "include": [
    "cypress/",
    "src/",
    "plugins/",
    "typings",
    "./docusaurusConfig.ts",
    "./rulesMeta.ts"
  ]
}
