{
  "name": "types",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "root": "packages/types",
  "sourceRoot": "packages/types/src",
  "implicitDependencies": ["ast-spec"],
  "targets": {
    "copy-ast-spec": {
      "dependsOn": [
        {
          "target": "build",
          "dependencies": true
        }
      ],
      "outputs": ["{projectRoot}/src/generated"],
      "cache": true
    },
    "build": {
      "dependsOn": [
        {
          "target": "copy-ast-spec"
        }
      ]
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
